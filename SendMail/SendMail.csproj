    <PackageReference Include="MailKit" Version="4.1.0" />
    <PackageReference Include="OpenCover" Version="4.7.1221" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.2.3" />
  </ItemGroup>

</Project>
